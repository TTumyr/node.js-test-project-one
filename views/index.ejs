<!DOCTYPE html>
<html lang="en">
  <%- include('./includes/header.ejs', {system}) %>
  <body>
    <div class="display1">
      <h3>User list</h3>
      <table>
        <thead>
          <tr>
            <th>User Id</th>
            <th>Username</th>
            <th>Email</th>
          </tr>
        </thead>
        <tbody>
          <% users.forEach((user) => { %>
          <tr>
            <td>
              <%= user._id %>
            </td>
            <td>
              <%= user.username %>
            </td>
            <td>
              <%= user.email %>
            </td>
          </tr>
          <% }) %>
        </tbody>
      </table>

      <div>
        <a href="/login">user login</a>
      </div>
      <div>
        <a href="/register-user">register user</a>
      </div>
    </div>
    <%- include('./includes/scripts.ejs') %> <%-
    include('./includes/footer.ejs') %>
  </body>
</html>
